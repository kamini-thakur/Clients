{% comment %}ly_global_begin{% endcomment %}{% include 'ly-global' %}{% comment %}ly_global_end{% endcomment %}<ul class="social">
  {%- if settings.social-facebook-link != blank -%}
    {% include 'social-link', id: 'facebook', title: 'Facebook', url: settings.social-facebook-link %}
  {%- endif -%}

  {%- if settings.social-twitter-link != blank -%}
    {% include 'social-link', id: 'twitter', title: 'Twitter', url: settings.social-twitter-link %}
  {%- endif -%}

  {%- if settings.social-google-link != blank -%}
    {% include 'social-link', id: 'google', title: 'Google+', url: settings.social-google-link %}
  {%- endif -%}

  {%- if settings.social-pinterest-link != blank -%}
    {% include 'social-link', id: 'pinterest', title: 'Pinterest', url: settings.social-pinterest-link %}
  {%- endif -%}

  {%- if settings.social-instagram-link != blank -%}
    {% include 'social-link', id: 'instagram', title: 'Instagram', url: settings.social-instagram-link %}
  {%- endif -%}

  {%- if settings.social-vimeo-link != blank -%}
    {% include 'social-link', id: 'vimeo', title: 'Vimeo', url: settings.social-vimeo-link %}
  {%- endif -%}

  {%- if settings.social-youtube-link != blank -%}
    {% include 'social-link', id: 'youtube', title: 'YouTube', url: settings.social-youtube-link %}
  {%- endif -%}

