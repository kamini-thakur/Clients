{% comment %}ly_global_begin{% endcomment %}{% include 'ly-global' %}{% comment %}ly_global_end{% endcomment %}{% layout none %}
<!doctype html>
<!--[if lt IE 7]><html class="no-js lt-ie10 lt-ie9 lt-ie8 lt-ie7" lang="en"> <![endif]-->
<!--[if IE 7]><html class="no-js lt-ie10 lt-ie9 lt-ie8" lang="en"> <![endif]-->
<!--[if IE 8]><html class="no-js ie8 lt-ie10 lt-ie9" lang="en"> <![endif]-->
<!--[if IE 9 ]><html class="no-js ie9 lt-ie10"> <![endif]-->
<!--[if (gt IE 9)|!(IE)]><!--> <html class="no-js"> <!--<![endif]-->
<head>

  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">

  <title>
    {{ langify_shop_name }}
  </title>

  {% if langify_description %}
    <meta name="description" content="{{ langify_description | escape }}" />
  {% endif %}

  {% if settings.favicon %}
    <link rel="shortcut icon" href="{% comment %}ly_asset_replace_for_[ 'favicon.png' | asset_url ]_begin{% endcomment %}{% assign ly_asset = 'favicon.png' %}{% include 'ly-asset' with ly_asset %}{{ ly_translation  }}{% comment %}ly_asset_replace_end{% endcomment %}" type="image/png" />
  {% endif %}

  <link rel="canonical" href="{{ canonical_url }}" />
  <meta name="viewport" content="width=device-width,initial-scale=1" />

  {{ content_for_header }}

  <!-- Theme CSS -->
  {% comment %}ly_asset_replace_for_[ 'theme.scss.css' | asset_url | stylesheet_tag ]_begin{% endcomment %}{% assign ly_asset = 'theme.scss.css' %}{% include 'ly-asset' with ly_asset %}{{ ly_translation  | stylesheet_tag }}{% comment %}ly_asset_replace_end{% endcomment %}

  {% if settings.custom-css != blank %}
    <style>
      {{ settings.custom-css }}
    </style>
  {% endif %}

  <!-- Third Party JS Libraries -->
  {% comment %}ly_asset_replace_for_[ 'modernizr-2.8.2.min.js' | asset_url | script_tag ]_begin{% endcomment %}{% assign ly_asset = 'modernizr-2.8.2.min.js' %}{% include 'ly-asset' with ly_asset %}{{ ly_translation  | script_tag }}{% comment %}ly_asset_replace_end{% endcomment %}

  <script>
    Theme = {}
  </script>
</head>

<body class="template-{{ template | replace: '.', ' ' | truncatewords: 1, '' | handle }}">

  <div class="password-page main-content">
    <div class="password-page-header">
      <div class="password-page-inner">
        <a href="#modal-login" class="admin-login-modal">
          {% comment %}ly_i18n_replace_for_[ 'general.password_page.password_link' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.password_page.password_link' | t }}{% endcapture %}{% include 'ly-static-string' with '6185890' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}
        </a>
      </div>
    </div>

    <div class="password-page-content">
      <div class="password-page-inner">
        <div class="password-page-logo">
          <header class="main-header" role="banner">
            <div class="password-branding">
              <a class="logo-link" href="/">
                <h1>{{ langify_shop_name }}</h1>
              </a>
            </div>
          </header>
        </div>
        <div class="password-page-signup-form">
          <div class="password-page-form-header">
            <h2>{% comment %}ly_i18n_replace_for_[ 'general.password_page.opening_soon' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.password_page.opening_soon' | t }}{% endcapture %}{% include 'ly-static-string' with '6185891' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</h2>

            {% unless shop.password_message == blank %}
              <p class="password-page-message">
                {{ shop.password_message }}
              </p>
            {% endunless %}
          </div>

          {% if settings.mailing-list %}
            <form action="{{ settings.mailing-list-action-url }}" method="post" name="mc-embedded-subscribe-form" class="validate" target="_blank">
              <p class="form-title">{% comment %}ly_i18n_replace_for_[ 'general.password_page.signup_form_heading' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.password_page.signup_form_heading' | t }}{% endcapture %}{% include 'ly-static-string' with '6185892' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</p>

              <div class="password-page-field-wrap">
                <input class="password-page-input" name="EMAIL" type="email" placeholder="{% comment %}ly_i18n_replace_for_[ 'general.password_page.signup_form_email_placeholder' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.password_page.signup_form_email_placeholder' | t }}{% endcapture %}{% include 'ly-static-string' with '6185893' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}">
                <input class="submit" name="subscribe" type="submit" value="{% comment %}ly_i18n_replace_for_[ 'general.password_page.signup_form_submit' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.password_page.signup_form_submit' | t }}{% endcapture %}{% include 'ly-static-string' with '6185894' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}">
                <input type="hidden" name="contact[tags]" value="prospect, password page">
              </div>
            </form>
          {% else %}
            {% form 'customer', class: 'newsletter-signup' %}
              {{ form.errors | default_errors }}

              {% if form.posted_successfully? %}
                <p class="form-success">{% comment %}ly_i18n_replace_for_[ 'general.password_page.signup_form_success' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.password_page.signup_form_success' | t }}{% endcapture %}{% include 'ly-static-string' with '6185895' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</p>
              {% else %}
                <p class="form-title">{% comment %}ly_i18n_replace_for_[ 'general.password_page.signup_form_heading' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.password_page.signup_form_heading' | t }}{% endcapture %}{% include 'ly-static-string' with '6185892' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</p>

                <div class="password-page-field-wrap">
                  <input class="password-page-input" type="text" name="contact[email]" id="email" placeholder="{% comment %}ly_i18n_replace_for_[ 'general.password_page.signup_form_email_placeholder' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.password_page.signup_form_email_placeholder' | t }}{% endcapture %}{% include 'ly-static-string' with '6185893' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}">
                  <input class="submit" name="commit" type="submit" value="{% comment %}ly_i18n_replace_for_[ 'general.password_page.signup_form_submit' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.password_page.signup_form_submit' | t }}{% endcapture %}{% include 'ly-static-string' with '6185894' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}">
                  <input type="hidden" name="contact[tags]" value="prospect, password page">
                </div>
              {% endif %}
            {% endform %}
          {% endif %}
        </div>
      </div>
    </div>

    <div class="password-page-footer" role="contentinfo">
      <div class="password-page-inner">
        {% capture shopify %}
          <a class="shopify-link" href="//www.shopify.com/" target="_blank" title="<ly-as-6185889>Create your own online store with Shopify</ly-as-6185889>">
            <span class="shopify-name"><ly-as-6185887>Shopify</ly-as-6185887></span>
          </a>
        {% endcapture %}
        {% comment %}ly_i18n_replace_for_[ 'general.password_page.powered_by_shopify_html' | t: shopify: shopify ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.password_page.powered_by_shopify_html' | t: shopify: shopify }}{% endcapture %}{% include 'ly-static-string' with '6185897' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% assign replacement = shopify  %}{% assign ly_template = ly_template | replace: '[[shopify]]', replacement %}{% if escape_content %}{{ ly_template}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}
      </div>
    </div>

  </div>

  <div id="modal-login" class="password-page-modal-wrapper">
    <div class="password-page-modal">
      <div class="module header">
        <p class="form-title">{% comment %}ly_i18n_replace_for_[ 'general.password_page.login_form_heading' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.password_page.login_form_heading' | t }}{% endcapture %}{% include 'ly-static-string' with '6185898' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</p>
      </div>

      {% form 'storefront_password' %}
        {{ form.errors | default_errors }}
        <div class="password-page-field-wrap" {% if form.errors %}data-open-modal{% endif %}>
          <input class="password-page-input" type="password" name="password" id="password" placeholder="{% comment %}ly_i18n_replace_for_[ 'general.password_page.login_form_password_label' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.password_page.login_form_password_label' | t }}{% endcapture %}{% include 'ly-static-string' with '6185899' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}" autofocus>
          <input class="submit" name="submit" type="submit" value="{% comment %}ly_i18n_replace_for_[ 'general.password_page.login_form_submit' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.password_page.login_form_submit' | t }}{% endcapture %}{% include 'ly-static-string' with '6185900' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}">
        </div>
      {% endform %}
      <p class="admin-login">
        {% comment %}ly_i18n_replace_for_[ 'general.password_page.admin_link_question' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.password_page.admin_link_question' | t }}{% endcapture %}{% include 'ly-static-string' with '6185901' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}
        <a href="/admin" class="admin-link">{% comment %}ly_i18n_replace_for_[ 'general.password_page.admin_link_direction' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.password_page.admin_link_direction' | t }}{% endcapture %}{% include 'ly-static-string' with '6185902' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</a>
      </p>
    </div>
  </div>

  <!-- Scripts -->
  {% comment %}ly_asset_replace_for_[ 'jquery-1.11.1.min.js' | asset_url | script_tag ]_begin{% endcomment %}{% assign ly_asset = 'jquery-1.11.1.min.js' %}{% include 'ly-asset' with ly_asset %}{{ ly_translation  | script_tag }}{% comment %}ly_asset_replace_end{% endcomment %}
  {{ 'api.jquery.js' | shopify_asset_url | script_tag }}

  {% comment %}ly_asset_replace_for_[ 'plugins.js'  | asset_url | script_tag ]_begin{% endcomment %}{% assign ly_asset = 'plugins.js' %}{% include 'ly-asset' with ly_asset %}{{ ly_translation  | script_tag }}{% comment %}ly_asset_replace_end{% endcomment %}
  {% comment %}ly_asset_replace_for_[ 'theme.js'  | asset_url | script_tag ]_begin{% endcomment %}{% assign ly_asset = 'theme.js' %}{% include 'ly-asset' with ly_asset %}{{ ly_translation  | script_tag }}{% comment %}ly_asset_replace_end{% endcomment %}

</body>
</html>
